<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when checkbox is checked -->
    <item android:drawable="@drawable/checkbox_checked" android:state_checked="true" android:color="@color/white" />
    <!-- State when checkbox is unchecked -->
    <item android:drawable="@drawable/checkbox_unchecked" android:state_checked="false" android:color="@color/white" />
</selector>
